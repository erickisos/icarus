{
	"name": "vscode-irc",
	"displayName": "IRC",
	"description": "Open an IRC channel in the editor",
	"version": "0.1.0",
	"publisher": "olivier-grech",
	"repository": {
		"type": "git",
		"url": "https://github.com/olivier-grech/vscode-irc"
	},
	"bugs": {
		"url": "https://github.com/olivier-grech/vscode-irc/issues"
	},
	"engines": {
		"vscode": "^1.4.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.openIrc"
	],
	"main": "./out/extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.openIrc",
				"title": "Open IRC",
				"category": "IRC"
			}
		],
		"languages": [
			{
				"id": "irc",
				"aliases": [
					"IRC"
				],
				"extensions": [
					".irc"
				]
			}
		],
		"grammars": [
			{
				"language": "irc",
				"path": "./irc-syntax.json",
				"scopeName": "source.irc"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"tslint": "^3.8.1",
		"typescript": "^2.0.3",
		"vscode": "^1.0.0",
		"@types/node": "^6.0.40",
		"@types/mocha": "^2.2.32"
	},
	"dependencies": {
		"irc": "^0.5.2"
	}
}
